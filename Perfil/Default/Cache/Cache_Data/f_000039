 Object.keys(graph);\n\tfor (let len = models.length, i = 0; i < len; i++) {\n\t\tconst toModel = models[i];\n\t\tconst node = graph[toModel];\n\n\t\tif (node.parent === null) {\n\t\t\t// No possible conversion, or this node is the source model.\n\t\t\tcontinue;\n\t\t}\n\n\t\tconversion[toModel] = wrapConversion(toModel, graph);\n\t}\n\n\treturn conversion;\n};\n\n"],"names":["cssKeywords","reverseKeywords","key","Object","keys","convert","rgb","channels","labels","hsl","hsv","hwb","cmyk","xyz","lab","lch","hex","keyword","ansi16","ansi256","hcg","apple","gray","model","module","exports","length","defineProperty","value","h","r","g","b","min","Math","max","delta","l","s","rdif","gdif","bdif","v","diff","diffc","c","w","k","m","y","currentClosestKeyword","reversed","currentClosestDistance","Infinity","distance","x","z","a","t2","t3","val","t1","i","smin","lmin","sv","f","floor","p","q","t","sl","vmin","wh","bl","ratio","n","y2","x2","z2","hr","atan2","PI","sqrt","cos","sin","args","saturation","round","ansi","color","mult","rem","integer","string","toString","toUpperCase","substring","match","colorString","split","map","char","join","parseInt","chroma","hue","grayscale","pure","hi","mg","conversions","route","models","forEach","fromModel","routes","routeModels","toModel","fn","wrapRounded","wrappedFn","arg0","result","len","conversion","raw","wrapRaw","graph","deriveBFS","buildGraph","parent","queue","current","pop","adjacents","adjacent","node","unshift","wrapConversion","path","cur","link","from","to"],"sourceRoot":""}